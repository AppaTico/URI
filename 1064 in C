#include <stdio.h>

int main()
{
  int contador = 0;
  float valor;
  float m = 0;
  float media;
  int i;
  for (i = 0; i < 6; i++)
  {
    scanf("%f", &valor);
    if(valor > 0)
    {
      contador++;
      m = m + valor;
      media = m / contador;
    }
  }
  printf("%d valores positivos\n", contador);
  printf("%.1f\n", media);

  return 0;
}
