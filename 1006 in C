#include <stdio.h>

int main() {
double pA = 2;
double pB = 3;
double pC = 5;
double a, b, c;
scanf("%lf %lf %lf", &a, &b, &c);
double A = pA * a;
double B = pB * b;
double C = pC * c;
double media = A + B + C;
double z = pA + pB + pC;
double MEDIA = media / z;
printf("MEDIA = %.1lf\n", MEDIA);

  return 0;
}
